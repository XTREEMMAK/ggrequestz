{
  "name": "gg-requestz",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "db:init": "node scripts/init-database.js init",
    "db:setup": "node scripts/setup-postgres.js setup",
    "db:warm": "node scripts/init-database.js warm",
    "db:stats": "node scripts/init-database.js stats",
    "db:migrate": "node scripts/migration-manager.js migrate",
    "db:migrate:status": "node scripts/migration-manager.js status",
    "db:migrate:list": "node scripts/migration-manager.js list",
    "db:migrate:validate": "node scripts/migration-manager.js validate",
    "games:update-slugs": "node scripts/update-game-slugs.js"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "terser": "^5.43.1",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "@iconify/svelte": "^5.0.1",
    "bcrypt": "^5.1.1",
    "canvas-confetti": "^1.9.3",
    "cookie": "^0.6.0",
    "dotenv": "^17.2.1",
    "jose": "^5.10.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "redis": "^5.8.0",
    "typesense": "^1.8.2"
  }
}
